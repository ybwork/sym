{% extends 'base.html.twig' %}

{% block title %}Hello{% endblock %}

{% block body %}
	<div class="example-wrapper">
	    <h1>Shared urls list:</h1>
	    
	    <ul>
            {% for url in urls %}
                <li><a target="_blank" href="{{ url.getName }}">{{ url.getShortName }}</a></li>
            {% endfor %}
	    </ul>
	</div>
{% endblock %}
